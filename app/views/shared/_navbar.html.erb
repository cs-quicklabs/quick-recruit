<% if current_user.admin? %>
  <%= render "shared/navbar/admin" %>
<% elsif current_user.recruiter? %>
  <%= render "shared/navbar/recruiter" %>
<% elsif current_user.data? %>
  <%= render "shared/navbar/data" %>
<% elsif current_user.interviewer? %>
  <%= render "shared/navbar/interviewer" %>
<% else %>
  <%= render "shared/navbar/default" %>
<% end %>
